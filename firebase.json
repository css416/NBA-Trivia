{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**gg.psd**",
      "**Testing.js**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
          "source": "*",
          "headers": [
              {
                  "key": "Access-Control-Allow-Origin",
                  "value": "*"
              }
          ]
      }
    ]
  }
}
